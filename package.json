        "@fortawesome/free-solid-svg-icons": "5.13.0",
        "@fortawesome/react-fontawesome": "0.1.9",
        "@mailchimp/mailchimp_marketing": "^3.0.69",
        "axios": "0.19.2",
        "eslint-config-prettier": "7.1.0",
        "eslint-plugin-prettier": "3.3.0",
        "hover.css": "2.3.2",
        "html-react-parser": "0.14.0",
        "jsonp": "^0.2.1",
        "prettier": "^2.2.1",
        "querystring": "^0.2.1",
        "react": "16.13.0",
        "react-dark-mode-toggle": "0.2.0",
        "react-dom": "16.13.0",
        "react-html-parser": "2.0.2",
        "react-mailchimp-subscribe": "^2.1.3",
        "react-router-dom": "5.2.0",
        "react-scripts": "3.4.0",
        "react-select-search": "2.1.1",
        "react-spinners": "0.9.0",
        "react-toastify": "6.0.9",
        "sass": "^1.49.7",
        "styled-components": "5.1.1"
    },
    "devDependencies": {
        "@babel/plugin-transform-react-jsx-development": "7.10.1",
        "@storybook/addon-a11y": "5.3.14",
        "@storybook/addon-actions": "5.3.14",
        "@storybook/addon-backgrounds": "6.0.26",
        "@storybook/addon-centered": "5.3.14",
        "@storybook/addon-docs": "6.0.0-alpha.20",
        "@storybook/addon-essentials": "6.0.26",
        "@storybook/addon-knobs": "5.3.14",
        "@storybook/addon-notes": "5.3.14",
        "@storybook/addon-viewport": "5.3.14",
        "@storybook/react": "5.3.14",
        "@testing-library/jest-dom": "5.1.1",
        "@testing-library/react": "9.4.1",
        "@testing-library/user-event": "10.0.0",
        "@types/enzyme": "3.10.5",
        "@types/enzyme-adapter-react-16": "1.0.6",
        "@types/jest": "25.1.3",
        "@types/jsonp": "^0.2.1",
        "@types/node": "13.7.7",
        "@types/react": "16.9.23",
        "@types/react-dom": "16.9.5",
        "@types/react-html-parser": "2.0.1",
        "@types/react-mailchimp-subscribe": "^2.1.1",
        "@types/react-router-dom": "5.1.5",
        "@types/react-toastify": "4.1.0",
