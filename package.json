        "eslint": "6.8.0",
        "eslint-plugin-react": "7.18.3",
        "gh-pages": "2.2.0",
        "jest": "24.9.0",
        "react-docgen-typescript-loader": "3.6.0",
        "react-docgen-typescript-webpack-plugin": "1.1.0",
        "react-test-renderer": "16.13.0",
        "ts-jest": "25.2.1",
        "typescript": "3.8.3"
    },
    "engines": {
        "node": ">=12.0.0",
        "npm": ">=6.0.0"
    }
